$("ul.errors").html("")
<% if @food.errors.any? %>
  <% @food.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
  <% end %>
<% else %>
  $(".food-index").html("<%= escape_javascript(render 'index') %>")
  $("#food-modal").modal("hide")
<% end %>
